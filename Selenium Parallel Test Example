[TestFixture]
[Parallelizable(ParallelScope.All)] // Enables parallel execution
public class ParallelTestExample
{
    IWebDriver driver;

    [SetUp]
    public void SetUp()
    {
        driver = new ChromeDriver();
    }

    [Test]
    public void Test1()
    {
        driver.Navigate().GoToUrl("https://example.com");
        Assert.IsTrue(driver.Title.Contains("Example"));
    }

    [Test]
    public void Test2()
    {
        driver.Navigate().GoToUrl("https://another-example.com");
        Assert.IsTrue(driver.Title.Contains("Another"));
    }

    [TearDown]
    public void TearDown()
    {
        driver.Quit();
    }
}
